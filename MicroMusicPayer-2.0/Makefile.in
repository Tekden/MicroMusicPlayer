

# I dislike directory printing behaviour of make: it look's like an error message cause of formatting style of this feature.
MAKEFLAGS += --no-print-directory

.PHONY: all install install-strip uninstall clean



all:
	@cd ./mmp-2.0 ; $(MAKE) ;

install:
	@cd ./mmp-2.0 ; $(MAKE) install

install-strip:
	@cd ./mmp-2.0 ; $(MAKE) install-strip

uninstall:
	@cd ./mmp-2.0 ; $(MAKE) uninstall

clean:
	@cd ./mmp-2.0 ; $(MAKE) clean



